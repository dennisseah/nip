        "@types/sinon": "^10.0.13",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "4.31.2",
        "@typescript-eslint/parser": "4.31.2",
        "chai": "^4.3.6",
        "eslint": "6.8.0",
        "mocha": "9.1.1",
        "nyc": "^15.1.0",
